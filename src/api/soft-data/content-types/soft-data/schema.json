{
  "kind": "collectionType",
  "collectionName": "soft_datas",
  "info": {
    "singularName": "soft-data",
    "pluralName": "soft-datas",
    "displayName": "Софт: дата",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "appLink": {
      "type": "text"
    },
    "version": {
      "type": "string",
      "unique": false,
      "required": true
    },
    "otherImages": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "specifics": {
      "type": "richtext"
    },
    "tags": {
      "type": "richtext"
    },
    "isPopulate": {
      "type": "boolean"
    },
    "isRecomended": {
      "type": "boolean"
    },
    "soft_category_id": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::soft-category.soft-category",
      "inversedBy": "soft_data"
    }
  }
}
